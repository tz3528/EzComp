OPT_STR(
    mode,
    "time-pde",
    oneOfStrings({"time-pde"})
)
OPT_STR(
    method,
    "FDM",
    oneOfStrings({"FDM"})
)
OPT_INT(
    precisionExp10,
    0,
    any()
)
OPT_STR(
    timeVar,
    "t",
    any()
)